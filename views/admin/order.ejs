	<h3>Orders management</h3>
	<div class="container">
		<div class="table-responsive">
            <table class="table table-striped table-sm">
              <thead>
                <tr>
                  <th>#</th>
                  <th>Email</th>
                  <th>Name</th>
                  <th>Address</th>
                  <th>Phone</th>
                  <th>Total</th>
                  <th>Action</th>
                </tr>
              </thead>
              <tbody>
                <% order.forEach((entry) => { %>
                <tr>
                  <td><%= entry.id %></td>
                  <td><%= entry.email %></td>
                  <td><%= entry.name %></td>
                  <td><%= entry.address %></td>
                  <td><%= entry.phone %></td>
                  <td><%= entry.total %></td>
                  <td><a href="/admin/order?id=<%= entry.id %>" class="btn btn-info">View</a></td>
                </tr>
                <% }) %>
              </tbody>
            </table>
        </div>
    </div>